{
  "author": "Ismael Ghalimi <ismael@stoic.com>",
  "name": "@lr/formulajs",
  "description": "JavaScript implementation of most Microsoft Excel formula functions",
  "version": "2.0.1",
  "repository": {
    "type": "git",
    "url": "git@github.com:sutoiku/formula.js.git"
  },
  "main": "index.js",
  "bin": {
    "implementation-stats": "./bin/implementation-stats"
  },
  "dependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.5.2",
    "bessel": "0.2.0",
    "jStat": "1.0.6",
    "numeral": "1.5.3",
    "numeric": "1.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "blanket": "^1.1.6",
    "browserify": "^13.0.0",
    "cli-table": "^0.3.0",
    "coveralls": "^2.11.1",
    "istanbul": "*",
    "jshint": "^2.5.2",
    "mocha": "*",
    "mocha-lcov-reporter": "0.0.1",
    "mocha-spec-cov": "0.0.3",
    "should": "*",
    "watchify": "^3.7.0"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "prepublish": "mkdir -p dist && browserify index.js -o dist/index.js -t [ babelify --presets [ es2015 ] ]"
  },
  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules"
    }
  }
}
